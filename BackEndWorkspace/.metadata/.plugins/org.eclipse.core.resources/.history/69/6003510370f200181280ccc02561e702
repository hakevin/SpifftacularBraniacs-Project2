package spiff.brain.models;

public class FinanceUpdate {
	private int id;
	private double wage;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public double getWage() {
		return wage;
	}
	public void setWage(double wage) {
		this.wage = wage;
	}
	public FinanceUpdate(int id, double wage) {
		super();
		this.id = id;
		this.wage = wage;
	}
	public FinanceUpdate() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}
